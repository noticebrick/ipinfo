{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ipinfo",
  "main": "src/index.tsx",
  "compatibility_date": "2025-09-26",
  "observability": {
    "enabled": true
  },
  "assets": {
    "directory": "./public",
  },
  "routes": [
    {
      "pattern": "example.com",
      "custom_domain": true
    }
  ],
  "vars": { "IPINFO_TOKEN": "IPINFO_TOKEN" }
}
